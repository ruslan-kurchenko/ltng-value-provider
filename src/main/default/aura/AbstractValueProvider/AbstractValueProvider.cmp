<aura:component abstract="true" extensible="true">
    <aura:attribute name="type" type="String" required="true" />
    <aura:attribute name="values" type="String" required="true" />
    <aura:attribute name="name" type="String" default="$valueProvider" />

    <aura:handler name="init" value="{!this}" action="{!c.onInit}" />
</aura:component>